{
  "name": "kikis-jewelry-ecommerce",
  "version": "1.0.0",
  "description": "Full-stack e-commerce website for Kiki's Jewelry",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "setup:db": "cd backend && node scripts/setup-database.js",
    "lint": "cd frontend && npm run lint",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules",
    "reset": "npm run clean && npm run install:all"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "ecommerce",
    "jewelry",
    "nextjs",
    "nodejs",
    "stripe",
    "postgresql"
  ],
  "author": "Kiki's Jewelry",
  "license": "PRIVATE"
}